function displayChargedOptionInput() {
	/* var selectObject = document.orderInitForm.wrapping_type; */
	var selectObject = document.cartUpdateForm.wrapping_type;
	var isDisplay = false;
	if (selectObject != null) {
		for(i=0; i<selectObject.length;i++) {
			if (selectObject[i].checked && selectObject[i].value != "")
				isDisplay = true;
		}
	}
	if (document.getElementById("charged_option_input") != null) {
		if (isDisplay) {
    		document.getElementById("charged_option_input").style.display = 'inline';
			if (document.getElementById("charged_option_numbers") != null) {
    			document.getElementById("charged_option_numbers").style.display = 'none';
				var form = document.cartUpdateForm;
				var num = 0;
				for (var i = 0; i < form.elements.length; i++) {
				  if (form.elements[i].name.indexOf("order_num_string") != -1) {
		  				num += form.elements[i].value;
	  				}
				}
				if (num > 1) 
   					document.getElementById("charged_option_numbers").style.display = 'inline';
			}
		}
		else {
    		document.getElementById("charged_option_input").style.display = 'none';
			if (document.getElementById("charged_option_numbers") != null)
    			document.getElementById("charged_option_numbers").style.display = 'none';
		}
	}
}
if ( window.addEventListener ) {
	window.addEventListener("load", displayChargedOptionInput, false);
} else if (window.attachEvent) {
	window.attachEvent("onload", displayChargedOptionInput);
}


